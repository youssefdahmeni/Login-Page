{
  "name": "fastapi-vue-login-demo",
  "version": "1.0.0",
  "description": "A demo project for a login page using FastAPI and Vue.js",
  "scripts": {
    "serve": "vue-cli-service serve",
    "backend": "cd ../backend && python -m uvicorn app.main:app --reload",
    "dev": "concurrently \"npm run serve\" \"npm run backend\"",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "vue": "^3.0.0",
    "axios": "^0.21.1"
  },
  "devDependencies": {
    "@vue/cli-service": "^4.5.0",
    "concurrently": "^9.2.1"
  }
}